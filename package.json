{
  "name": "narrowcasting-frontend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint ./src --ext .vue,.js,.jsx,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore --max-warnings=1",
    "lint-fix": "eslint ./src --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore --fix",
    "format": "prettier --ignore-path .gitignore --check ./src/",
    "format-fix": "prettier --ignore-path .gitignore --write ./src/",
    "gen-client": "openapi --input ../aurora-core/build/swagger.json --output src/api/",
    "prepare": "husky install"
  },
  "dependencies": {
    "@primevue/themes": "^4.0.4",
    "@vee-validate/yup": "^4.13.2",
    "axios": "^1.7.4",
    "dotenv": "^16.4.5",
    "jwt-decode": "^4.0.0",
    "pinia": "^2.2.2",
    "primeicons": "^7.0.0",
    "primevue": "^4.0.4",
    "socket.io-client": "^4.7.5",
    "tailwind": "^4.0.0",
    "tailwindcss-primeui": "^0.3.4",
    "uuid": "^10.0.0",
    "vee-validate": "^4.13.2",
    "vue": "^3.4.38",
    "vue-router": "^4.4.3",
    "vue-timeago3": "^2.3.2",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.10.4",
    "@tsconfig/node18": "^18.2.4",
    "@types/lodash": "^4.17.7",
    "@types/node": "20.15.0",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-vue": "^5.1.2",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.20",
    "eslint": "8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-vue": "^9.27.0",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.9",
    "lodash": "^4.17.21",
    "npm-run-all": "^4.1.5",
    "nswag": "^14.1.0",
    "openapi-typescript-codegen": "0.29.0",
    "postcss": "^8.4.41",
    "prettier": "^3.3.3",
    "sass": "^1.77.8",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.5.4",
    "vite": "^5.4.1",
    "vite-plugin-chunk-split": "^0.5.0",
    "vue-tsc": "^2.0.29"
  }
}
