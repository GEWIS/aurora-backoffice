{
  "name": "narrowcasting-frontend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint ./src --ext .vue,.js,.jsx,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore --ignore-path .formatignore --max-warnings=0",
    "lint-fix": "eslint ./src --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore --ignore-path .formatignore",
    "format": "prettier --ignore-path .gitignore --ignore-path .formatignore --check ./src/",
    "format-fix": "prettier --ignore-path .gitignore --ignore-path .formatignore --write ./src/",
    "gen-client": "nswag openapi2tsclient /input:../narrowcasting-core/build/swagger.json /output:src/api/Client.ts",
    "prepare": "husky install"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-brands-svg-icons": "^6.5.1",
    "@fortawesome/free-regular-svg-icons": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/vue-fontawesome": "^3.0.6",
    "@vee-validate/yup": "^4.11.8",
    "axios": "^1.5.1",
    "dotenv": "^16.3.1",
    "jwt-decode": "^3.1.2",
    "pinia": "^2.1.7",
    "primeflex": "^3.3.1",
    "primeicons": "^6.0.1",
    "primevue": "^3.37.0",
    "sass": "^1.69.4",
    "socket.io-client": "^4.7.4",
    "uuid": "^9.0.1",
    "vee-validate": "^4.11.8",
    "vue": "^3.3.6",
    "vue-router": "^4.2.5",
    "yup": "^1.3.2"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.5.1",
    "@tsconfig/node18": "^18.2.2",
    "@types/lodash": "^4.14.200",
    "@types/node": "^20.8.8",
    "@types/uuid": "^9.0.6",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.52.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-vue": "^9.18.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "lodash": "^4.17.21",
    "npm-run-all": "^4.1.5",
    "nswag": "^14.0.2",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vite-plugin-mkcert": "^1.17.2",
    "vue-tsc": "^1.8.20"
  }
}
